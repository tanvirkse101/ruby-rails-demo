<% provide(:title, 'Manage members') %>
<div class="row">
    <div class="col-md-6">
        <h3>Members</h3>
        <% if @members.any? %>
          <ul class="users follow">
            <%= render partial: 'users/member', collection: @members %>
          </ul>
          <%= will_paginate @members, :param_name => 'members_page' %>
        <% end %>
    </div>
    <div class="col-md-6">
      <h3>Non members</h3>
      <% if @non_members.any? %>
        <ul class="users follow">
            <%= render partial: 'users/nonmember', collection: @nonmembers %>
        </ul>
        <%= will_paginate @non_members, :param_name => 'nonmembers_page' %>
      <% end %>
    </div>
</div>
